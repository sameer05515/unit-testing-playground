<div id="listCol" class="col-md-4">
  <h4>Files</h4>
  <ul class="list-group">
    <% files.forEach(file => { %>
    <li class="list-group-item <%= slug === file.slug ? 'active' : '' %>">
      <a href="/?slug=<%= file.slug %>" class="text-decoration-none <%= slug === file.slug ? 'text-white' : '' %>">
        <%= file.slug %>
      </a>
    </li>
    <% }); %>
  </ul>
</div>
